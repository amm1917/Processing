size(500,500);
//Horns
stroke(#D1BF67);
strokeWeight(20);
line(249, 230, 200, 180);
line(200, 180, 200, 150);
line(249, 230, 298, 180);
line(298, 180, 298, 150);
//Head
stroke(#0A0A0A);
strokeWeight(1);
fill(#8E4E0A);
ellipse(249, 234, 100, 130);
//Nose or Snout
fill(#8B7F72);
ellipse(249, 279, 60, 40);
//Nostrils
fill(#080807);
ellipse(239, 279, 15, 15);
ellipse(259, 279, 15, 15);
//Eyes
fill(#FFFFFF);
rectMode(CENTER);
rect(225, 220, 20, 10);
rect(273, 220, 20, 10);
//Eyeballs
fill(#1C1919);
ellipse(225, 220, 5, 5);
ellipse(273, 220, 5, 5);